<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="icu.junyao.crm.workbench.dao.ClueRemarkDao">
	<select id="getClueRemarksByClueId" resultType="ClueRemark">
        select * from tbl_clue_remark where clueId=#{clueId}
    </select>
  	<delete id="delete">
        delete from tbl_clue_remark where clueRemarkId=#{clueRemarkId}
    </delete>
</mapper>

































